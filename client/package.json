{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start:dev": "NODE_ENV=development next dev",
        "format": "prettier --write \"./**/*.{js,ts,jsx,tsx}\" ",
        "build": "cross-env GENERATE_SOURCEMAP=false next build",
        "start": "NODE_ENV=production next start",
        "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
        "test:e2e": "NODE_ENV=test jest --findRelatedTests /test/pagesE2E/ --runInBand --detectOpenHandles --forceExit",
        "test:watch": "NODE_ENV=test  jest --watch --runInBand --detectOpenHandles",
        "test:cov": "NODE_ENV=test  jest --coverage --runInBand --detectOpenHandles --forceExit"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.5.1",
        "axios": "^0.21.1",
        "global": "^4.4.0",
        "next": "10.1.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "^7.0.4",
        "react-redux": "^7.2.3",
        "universal-cookie": "^4.0.4"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.13.12",
        "@testing-library/react": "^11.2.6",
        "@types/jest": "^26.0.22",
        "@types/react": "^17.0.3",
        "@types/react-test-renderer": "^17.0.1",
        "@types/redux-mock-store": "^1.0.2",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
