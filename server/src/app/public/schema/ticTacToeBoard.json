      "description": "tic tac toe information about a game",
      "properties": {
            "id": {
                  "type": "string"
            },
            "startDate": {
                  "type": "string"
            },
            "lastStep": {
                  "type": "string"
            },
            "status": {
                  "type": "string"
            },
            "board": {
                  "type": "array",
                  "items": {
                        "type": "array",
