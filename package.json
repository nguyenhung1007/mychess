{
        "dependencies": {
                "husky": "^6.0.0"
        },
        "scripts": {
                "postinstall": "husky install",
                "build": "cp ./data/config/client.env ./client/config/.env.production && docker-compose up --build",
                "build:test": "cp ./data-dev/config/client.env ./client/config/.env.production && docker-compose -f docker-compose-build-test.yml up --build",
                "server:dev": "docker-compose --file ./dev/docker-compose-server-dev.yml up -d",
                "client:dev": "docker-compose --file ./dev/docker-compose-client-dev.yml up -d"
        },
        "name": "mygame-next-nest",
        "version": "1.0.0",
        "main": "index.js",
        "repository": "https://github.com/Heaty566/MyGame-NEXT-NEST.git",
        "author": "heaty566 <heaty566@gmail.com>",
        "license": "MIT",
        "hooks": {
                "pre-commit": "jest"
        }
}
